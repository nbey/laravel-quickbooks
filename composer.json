{
    "name": "jarvus/laravel-quickbooks",
    "description": "Laravel Quickbooks integration",
    "license": "MIT",
    "authors": [
        {
            "name": "Jani Cerar",
            "email": "jani.cerar@lifeonscreen.com",
            "homepage": "https://lifeonscreen.com"
        },
        {
            "name": "Chris Alfano",
            "email": "chris@jarv.us",
            "homepage": "https://jarv.us"
        },
        {
            "name": "Nafis Bey",
            "email": "nafis@jarv.us",
            "homepage": "https://jarv.us"
        }
    ],
    "homepage": "https://github.com/JarvusInnovations/laravel-quickbooks",
    "keywords": ["Laravel", "laravel-quickbooks"],
    "require": {
        "illuminate/support": "~11|~10|~9|~8|~7|^6.2|~5",
        "quickbooks/v3-php-sdk": "^6.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~9|~8|~7|~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Jarvus\\LaravelQuickBooks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jarvus\\LaravelQuickBooks\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jarvus\\LaravelQuickBooks\\LaravelQuickBooksServiceProvider"
            ],
            "aliases": {
                "laravel-quickbooks": "Jarvus\\LaravelQuickBooks\\Facades\\LaravelQuickBooks"
            }
        }
    }
}
